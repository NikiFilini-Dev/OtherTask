{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports":true,
    "baseUrl": "./src",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
//    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "lib": [
      "es2020", "dom", "dom.iterable"
    ],
    "downlevelIteration": true
  }
}