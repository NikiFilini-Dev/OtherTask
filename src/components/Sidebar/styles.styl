
.logoWrapper
  display flex
  align-items center
  margin-bottom 32px

.logo
  height 35px
  width 35px

.logoTitle
  font-family Gilroy
  font-style normal
  font-weight 600
  font-size 24px
  line-height 29px
  color #161938
  margin-left 9px

.groupTitle
  color #9BABC5
  font-size 16px
  font-weight 600
  font-family SF Pro Display
  margin-bottom 8px
  display flex
  align-items center
  margin-top 36px
  cursor pointer

.groupTitleIcon
  margin-right 12px
  transition transform .2s

.groupTitleIconOpen
  transform rotate(90deg)

.groupElement
  height 44px
  border-radius 5px
  margin-bottom 4px
  color black
  font-weight 600
  display flex
  align-items center
  cursor pointer

.groupElement.active
  color black
  background #f7f7f7

.groupElementIcon
  margin-right 14px
  margin-left 19px
