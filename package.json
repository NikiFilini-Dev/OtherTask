{
  "name": "othertodos",
  "productName": "OtherTodos",
  "version": "1.5.1",
  "description": "Best app for tasks",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "ef:publish": "electron-forge publish",
    "lint": "eslint . --fix --ext js --ext jsx",
    "pretty": "prettier --write \"./**/*.js*(x)\"",
    "pub:base": "git push && git push --tags && yarn ef:publish",
    "pub:patch": "yarn version --patch && yarn pub:base",
    "pub:minor": "yarn version --minor && yarn pub:base",
    "pub:major": "yarn version --major && yarn pub:base",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "lunavod",
    "email": "lunavod@bk.ru"
  },
  "license": "MIT",
  "config": {
    "electronRebuildConfig": {
      "extraModules": [
        "electron-vibrancy"
      ]
    },
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
